{
	"name": "pipeline_postgree_cosmosDB",
	"properties": {
		"activities": [
			{
				"name": "Copy data postgree",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "PostgreSqlSource",
						"query": "select productid, name from production.product where productid = 2"
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "insert",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "productid",
									"type": "Int32"
								},
								"sink": {
									"path": "$"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "PostgreSqlTable1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CosmosDbSqlApiCollection1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}